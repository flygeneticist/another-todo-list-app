<div id="signUpForm">
    <h3>Sign Up</h3>
    <form class="form" method="POST" onSubmit="/signup">
        <label name="email">Email</label>
        <input id="email" type="text" placeholder="you@domain.com"></input><br>
        <label name="password">Password</label>
        <input id="password" type="password"></input><br>
        <label name="passwordConfirm">Re-type Password</label>
        <input id="passwordComfirm" type="password"></input><br>
        <button id="signupButton" type="submit" class="btn btn-default">Submit</button>
    </form>
</div>
