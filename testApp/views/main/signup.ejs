<div id="signUpForm">
    <h3>Sign Up</h3>
    <form id="signupForm" class="form" method="POST" onsubmit="signupUser()">
        <label name="email">Email</label>
        <input id="email" type="email" autofocus="" placeholder="you@domain.com" required><br>
        <label name="password">Password</label>
        <input id="password" type="password" required><br>
        <label name="passwordConfirm">Re-type Password</label>
        <input id="passwordConfirm" type="password" required><br>
        <button id="signupButton" type="submit" class="btn btn-default">Submit</button>
    </form>
</div>
